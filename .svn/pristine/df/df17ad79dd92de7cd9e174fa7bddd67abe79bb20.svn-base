            transform-style: preserve-3d;
            height: 50px;
        }

        .navbar-alignit .navbar-brand {
            top: 50%;
            display: block;
            position: relative;
            height: auto;
            -webkit-transform: translate(0,-50%);
            transform: translate(0,-50%);
